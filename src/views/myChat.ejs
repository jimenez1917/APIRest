<%- include('./partials/head') %> 
<html lang="en">
  <head>
      <meta charset="UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
      <title>Document</title>
</head>
 <%if (chathistory.length>0){ %>
  <% chathistory.forEach(chat =>{ %>
      <div class="card" style="width: 18rem;">
        <div class="card-body">
          <h6 class="card-subtitle mb-2 text-muted"><%=chat.Username%></h6>
          <h5 class="card-title" color="blue"><%=chat.Message%></h5>
        </div>
          <% chat.Respuestas.forEach(respuesta =>{ %>
            <h6 class="card-title"><%=respuesta.UserResponse%></h6>
            <h9 class="card-text"><%=respuesta.Respuesta%></h9>
          <% }) %>
          </div> 
  <% }) %> 
<%} else { %>
  <p>No hay chat</p>
<% } %>